# Environment Configuration Example
# Copy this to .env.local or set as environment variables

# NVIDIA NIM API Configuration
NVIDIA_API_KEY=nvapi-YOUR_API_KEY_HERE

# Supabase Configuration  
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=sbp_YOUR_ANON_KEY_HERE

# Build Configuration
TRIAL_MODE=false

# Security Settings
ENABLE_CERTIFICATE_PINNING=true
ALLOW_HTTP_IN_DEBUG=false

# Development Settings (only for debug builds)
DEBUG_LOGGING=false